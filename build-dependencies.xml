<project name="Dependency Import" basedir=".">
  <!-- Properties used in compile_targets ANT target -->
  <property name="component.target" value="1.5" />
  <property name="component.bootclasspath" value="${java_1_5_bootclasspath}" />

    <!-- TopCoder dependencies -->
    <property name="base_exception.version" value="2.0.0"/>
    <property name="base_exception.jar.name" value="base_exception.jar"/>
    <property name="base_exception.path" value="base_exception/${base_exception.version}"/>
    <property name="base_exception.jar" value="${tcs_libdir}/${base_exception.path}/${base_exception.jar.name}"/>
    <property name="excel_utility.jar" value="${tcs_libdir}/excel_utility/2.0/excel_utility.jar"/>
    <property name="typesafe_enum.jar" value="${tcs_libdir}/typesafe_enum/1.1/typesafe_enum.jar"/>
    <property name="object_formatter.jar" value="${tcs_libdir}/object_formatter/1.0/object_formatter.jar"/>
    <property name="logging_wrapper.jar" value="${tcs_libdir}/logging_wrapper/2.0/logging_wrapper.jar"/>
    <property name="search_builder.jar" value="${tcs_libdir}/search_builder/1.4.0/search_builder_1.4_modified.jar"/>
    <property name="contest_service_facade.jar" value="${tcs_libdir}/contest_service_facade/1.1.0/contest_service_facade.jar"/>


    <property name="shared.jar" value="${tcs_libdir}/shared.jar"/>
    <property name="tcwebcommon.jar" value="${tcs_libdir}/tcwebcommon.jar"/>
    <property name="security.jar" value="${tcs_libdir}/security/security.jar"/>
    <property name="cockpitclient.jar" value="${tcs_libdir}/cockpit-client.jar"/>

  <!-- 3rd party dependencies -->
    <property name="servlet-api.jar" value="${ext_libdir}/servlet-api/servlet-api.jar"/>

    <property name="ejb-api.jar" value="${ext_libdir}/ejb-api/ejb-api-3.0.jar"/>

    <property name="javaee.jar" value="${ext_libdir}/j2ee/javaee.jar"/>
    <property name="jaxb.jar" value="${ext_libdir}/jaxb/2.1.7/jaxb-api.jar"/>
    <property name="aws-java" value="${ext_libdir}/aws-java-sdk/aws-java-sdk-1.0.004.jar"/>

  <!-- EJB/Persistence libraries -->
  <path id="ejb.libs">
      <fileset dir="${ext_libdir}/ejb-api">
          <include name="*.jar"/>
      </fileset>
  </path>

  <!-- Struts2 libraries -->
  <path id="struts.libs">
      <fileset dir="${ext_libdir}/struts">
          <include name="*.jar"/>
      </fileset>
  </path>

  <!-- Spring libraries -->
  <path id="spring.libs">
      <fileset dir="${ext_libdir}/spring">
          <include name="*.jar"/>
      </fileset>
  </path>

  <!-- JBoss libraries -->
  <path id="jboss.libs">
      <fileset dir="${ext_libdir}/jboss">
          <include name="*.jar"/>
      </fileset>
  </path>

  <path id="component.tcs-dependencies">
        <pathelement location="${shared.jar}"/>
        <pathelement location="${tcwebcommon.jar}"/>
        <pathelement location="${cockpitclient.jar}"/>
        <pathelement location="${security.jar}"/>
        <pathelement location="${base_exception.jar}"/>
        <pathelement location="${excel_utility.jar}"/>
        <pathelement location="${typesafe_enum.jar}"/>
        <pathelement location="${object_formatter.jar}" />
        <pathelement location="${logging_wrapper.jar}" />
        <pathelement location="${base_exception.jar}" />
        <pathelement location="${search_builder.jar}" />
        <pathelement location="${contest_service_facade.jar}" />
  </path>

  <path id="component.3rdParty-dependencies">
        <path refid="struts.libs"/>
        <path refid="spring.libs"/>
        <path refid="ejb.libs"/>
        <path refid="jboss.libs"/>
        <pathelement location="${servlet-api.jar}"/>
        <pathelement location="${jaxb.jar}"/>
        <pathelement location="${javaee.jar}"/>
        <pathelement location="${aws-java}"/>

  </path>

</project>
