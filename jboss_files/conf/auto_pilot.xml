<CMConfig>
    <!-- This namespace is used to configure the ActiveAutoPilotSource through default constructor -->
    <Config name="com.topcoder.management.phase.autopilot.impl.ActiveAutoPilotSource">
        <Property name="com/topcoder/management/project/ProjectManager">
            <Property name="type">
                <Value>com.topcoder.management.project.studio.ContestManagerProjectAdapter</Value>
            </Property>
            <Property name="params">
                <Property name="param1">
                    <Property name="type">
                        <Value>String</Value>
                    </Property>
                    <Property name="value">
                        <Value>direct/ContestManagerBean/local</Value>
                    </Property>
                </Property>
            </Property>
        </Property>
    </Config>

    <!-- This namespace is used to configure the DefaultProjectPilot through default constructor -->
    <Config name="com.topcoder.management.phase.autopilot.impl.DefaultProjectPilot">
        <Property name="com/topcoder/management/phase/PhaseManager">
            <Property name="type">
                <Value>com.topcoder.management.phase.clientcockpit.CockpitPhaseManager</Value>
            </Property>
            <Property name="params">
                <Property name="param1">
                    <Property name="type">
                        <Value>String</Value>
                    </Property>
                    <Property name="value">
                        <Value>com.topcoder.management.phase.clientcockpit.CockpitPhaseManager</Value>
                    </Property>
                </Property>
            </Property>
        </Property>
    </Config>

    <!-- This namespace is used to configure the AutoPilot through default constructor -->
    <Config name="com.topcoder.management.phase.autopilot.AutoPilot">
        <Property name="com/topcoder/management/phase/autopilot/AutoPilotSource">
            <Property name="type">
                <Value>com.topcoder.widgets.assembly.CockpitAutoPilotSource</Value>
            </Property>
            <Property name="params">
                <Property name="param1">
                    <Property name="type">
                        <Value>String</Value>
                    </Property>
                    <Property name="value">
                        <Value>com.topcoder.management.phase.autopilot.impl.ActiveAutoPilotSource</Value>
                    </Property>
                </Property>
                <Property name="param2">
                    <Property name="type">
                        <Value>String</Value>
                    </Property>
                    <Property name="value">
                        <Value>com.topcoder.management.project.ProjectManager</Value>
                    </Property>
                </Property>
                <Property name="param3">
                    <Property name="type">
                        <Value>String</Value>
                    </Property>
                    <Property name="value">
                        <Value>Active - Public</Value>
                    </Property>
                </Property>
                <Property name="param4">
                    <Property name="type">
                        <Value>String</Value>
                    </Property>
                    <Property name="value">
                        <Value>Autopilot Option</Value>
                    </Property>
                </Property>
                <Property name="param5">
                    <Property name="type">
                        <Value>String</Value>
                    </Property>
                    <Property name="value">
                        <Value>On</Value>
                    </Property>
                </Property>
            </Property>
        </Property>
        <Property name="com/topcoder/management/phase/autopilot/ProjectPilot">
            <Property name="type">
                <Value>com.topcoder.management.phase.autopilot.impl.DefaultProjectPilot</Value>
            </Property>
        </Property>
    </Config>

    <!-- This namespace is used to configure the AutoPilotJob through default constructor -->
    <Config name="com.topcoder.management.phase.autopilot.AutoPilotJob">
        <Property name="Operator">
            <Value>TC Direct Cockpit AutoPilot</Value>
        </Property>
    </Config>

    <Config name="com.topcoder.management.phase.autopilot.AutoPilotJob.factory">
        <Property name="com/topcoder/management/phase/autopilot/AutoPilot">
            <Property name="type">
                <Value>com.topcoder.management.phase.autopilot.AutoPilot</Value>
            </Property>
        </Property>
    </Config>

